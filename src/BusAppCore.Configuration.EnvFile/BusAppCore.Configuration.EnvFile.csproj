<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net6.0</TargetFrameworks>
		<!--<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>-->

		<Version>0.5.0.0</Version>
		<PackageVersion>0.5.0</PackageVersion>
		<LangVersion>10.0</LangVersion>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<OutputPath>..\..\bin\$(Configuration)\</OutputPath>

		<RootNamespace>Microsoft.Extensions.Configuration</RootNamespace>
		<AssemblyName>BusAppCore.Configuration.EnvFile</AssemblyName>
		<AssemblyTitle>BusAppCore.Configuration.EnvFile</AssemblyTitle>
		<Description>Reads and manages configuration values from a .env file</Description>

		<Company>Eniola Benjamin-Ade</Company>
		<Product>BusAppCore Utilities</Product>
		<Copyright>Copyright © 2022</Copyright>
		<Authors>Eniola Benjamin-Ade</Authors>
	</PropertyGroup>
	
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Configuration.FileExtensions" Version="6.0.0" />
	</ItemGroup>
</Project>
